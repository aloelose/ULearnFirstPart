using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace MainQuestionOfTheUniverse
{
    internal class MainQuestionOfTheUniverse
    {
        public static void Main()
        {
            Print(GetSquare(42));
        }
        private static void Print(double number)
        {
            Console.WriteLine(number);
        }

        private static double GetSquare(int number)
        {
            return Math.Pow(number, 2);
        }
    }
}
