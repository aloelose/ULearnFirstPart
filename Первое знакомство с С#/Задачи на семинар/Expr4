using System;

internal class Program
{
   
    public static void Main()
    {
        int number = 9;
        int x = 2;
        int y = 3;
        int count = 0;
        for (int i = 0; i < number; i++)
        {
            if ((i % x == 0) || (i % y == 0))
            {
                count++;
            }
        }
        Console.WriteLine(count);
        
    }


}
