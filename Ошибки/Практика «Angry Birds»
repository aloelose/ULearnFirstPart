using System;

namespace AngryBirds;

public static class AngryBirdsTask
{
    public static double FindSightAngle(double v, double distance)
    {
        try
        {
            double g = 9.8;
            double angleOfReach = 0.5 * Math.Asin((g * distance) / Math.Pow(v, 2));
            return angleOfReach;
        }
        catch
        {
            return double.NaN;
        }
        
    }
}
